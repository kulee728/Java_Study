package com.logger.singleton;

public class MainEx implements Runnable{
	private SingletonEx seInstance;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//SingletonEx [] se = new SingletonEx[10];
		Thread t1 = new Thread(new MainEx());
		Thread t2 = new Thread(new MainEx());
		Thread t3 = new Thread(new MainEx());
		Thread t4 = new Thread(new MainEx());
		Thread t5 = new Thread(new MainEx());
		t1.start();
		t2.start();
		t3.start();
		t4.start();
		t5.start();
	}

	@Override
	public void run() {
		int tid = this.hashCode()%10000;
		seInstance = SingletonEx.getInstance();
		System.out.println("Thread "+tid+" is on the way!");
		seInstance.logInfo(String.valueOf(tid));
		System.out.println("Instance hashcode : "+seInstance.hashCode()%100000);
	}

}
