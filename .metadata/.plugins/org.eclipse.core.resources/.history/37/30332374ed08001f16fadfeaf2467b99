package com.design.behaviorPattern.observer.observer;

public interface Observer {
	void update (String stockName,double price);
	public void sellStocks(int amount) ;
	public void buyStocks(String stockName) ;
	public void buyStocks(String stockName, int amount) ;
	public void sellStocks(String stockName) ;
	public void sellStocks(String stockName, int amount);
}
